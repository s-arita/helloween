<%= form_for(@photo) do |f| %>
  <% if @photo.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@photo.errors.count, "error") %> prohibited this image from being saved:</h2>

      <ul>
      <% @photo.errors.full_messages.each do |message| %>
